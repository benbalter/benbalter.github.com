  "repository": {
    "type": "git",
    "url": "git+https://github.com/benbalter/benbalter.github.com.git"
  },
  "scripts": {
    "build": "npm run webpack",
    "build:og-images": "tsx script/generate-og-images.ts",
    "build:related-posts": "tsx script/build-related-posts.ts",
    "dev": "next dev",
    "astro:dev": "astro dev",
    "astro:build": "astro build",
    "astro:preview": "astro preview",
    "astro:check": "astro check --tsconfig tsconfig.astro.json",
    "next:build": "next build && next-sitemap --config next-sitemap.config.cjs && node script/generate-redirects.mjs && tsx script/generate-feeds.mjs",
    "next:start": "serve out",
    "lint": "npm run lint-js && npm run lint-json && npm run lint-md",
    "lint-js": "xo .remarkrc.js --ignore-pattern !.remarkrc.js --fix",
    "lint-json": "eslint . --ext json --fix",
    "lint-md": "remark . && markdownlint-cli2 --fix \"**/*.{md,mdx}\" \"!node_modules\" \"!.github\"",
    "lint-text": "textlint *.md _posts/*.md _resume_positions/*.md --fix --dry-run",
    "lint-yaml": "yamllint -c .yamllint.yml .github/workflows/ci.yml *.yml _data/*.yml",
    "super-linter": "script/super-linter",
    "test": "script/cibuild",
    "test:e2e": "playwright test",
    "test:e2e:astro": "playwright test --config=playwright-astro.config.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:vitest:ui": "vitest --ui",
    "test:vitest:coverage": "vitest run --coverage",
    "webpack": "webpack"
  },
  "type": "module",
  "version": "1.0.0",
  "xo": {
    "esnext": false,
    "space": true
  }
}
